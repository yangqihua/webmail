{
	"name": "afterlogic/aurora-core",
	"version": "dev-master",
	"source": {
		"type": "git",
		"url": "https://github.com/afterlogic/aurora-core",
		"reference": "master"
	},
	"dist": {
		"type": "zip",
		"url": "https://github.com/afterlogic/aurora-core/archive/master.zip",
		"reference": "master"
	},
	"autoload": {
		"psr-4" : {
			"AuroraPlatform\\": "system/"
		}
	},
	"type": "aurora-core",
	"scripts": {
		"post-update-cmd": [
			"AuroraPlatform\\Installer::postInstall"
		]
	},
	"require": {
		"google/recaptcha": "~1.1",
		"sabre/dav": "3.0.*",
		"sabre/uri": "1.0.*",
		"knplabs/knp-snappy": "*",
		"symfony/filesystem": "2.8",
		"masterexploder/PHPThumb": "2.1",
		"PHPGangsta/GoogleAuthenticator" : "dev-master",
		"phpzip/phpzip": ">=2.0.7",
		"tijsverkoyen/css-to-inline-styles": "1.3.*",
		"afterlogic/dav": "dev-master",
		"afterlogic/mailso": "dev-master",
		"anahkiasen/underscore-php": "*",
		"phpmailer/phpmailer": "~5.2"
	}
}
